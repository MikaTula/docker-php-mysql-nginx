{
	"name": "mk/project",
	"type": "project",
	"description": "mk",
	"require": {
		"php": "^7.4",
		"ext-fileinfo": "*",
		"ext-json": "*",
		"ext-mysqli": "*",
		"ext-zip": "*",
		"chrome-php/chrome": "^1.7",
		"guzzlehttp/guzzle": "*",
      "ext-pdo": "*"
    },
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
